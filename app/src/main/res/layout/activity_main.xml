<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"



    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TestSerial!"
        />

    <Button
        android:id="@+id/btn_register"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="register"
        android:visibility="visible"/>

    <Button
        android:id="@+id/btn_serach"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="serach"
        android:visibility="visible" />


    <Button
        android:id="@+id/btn_deleteOne"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="deleteOne"
        android:visibility="visible"/>


    <Button
        android:id="@+id/btn_deleteALL"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="deleteAll"
        android:visibility="visible"/>


    <Button
        android:id="@+id/btn_testSend"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_send"
        android:visibility="visible"/>

    <Button
        android:id="@+id/btn_testSend1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_send1"
        android:visibility="visible"/>

    <Button
        android:id="@+id/btn_testSend2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="test_send2"
        android:visibility="visible"/>




</LinearLayout>